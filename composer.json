{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.109",
        "bdelespierre/php-kmeans": "dev-master",
        "bugsnag/bugsnag-laravel": "^2.0",
        "caouecs/laravel-lang": "~3.0",
        "dusterio/laravel-aws-worker": "^0.1.17",
        "fideloper/proxy": "^4.0",
        "gasparesganga/php-shapefile": "^2.4",
        "geocoder-php/arcgis-online-provider": "^4.0",
        "geocoder-php/bing-maps-provider": "^4.0",
        "geocoder-php/cache-provider": "^4.0",
        "geocoder-php/google-maps-provider": "^4.1",
        "geocoder-php/nominatim-provider": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.8.*",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "league/csv": "^9.1",
        "league/flysystem-aws-s3-v3": "~1.0",
        "mjaschen/phpgeo": "^2.0",
        "php-http/guzzle6-adapter": "^1.1",
        "predis/predis": "^1.1",
        "toin0u/geocoder-laravel": "^4.0",
        "tom-lingham/searchy": "2.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "GeoLV\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "toin0u/geocoder-laravel",
                "tom-lingham/searchy"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
